<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>Ex1</servlet-name>
        <servlet-class>Ex1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ex2-1</servlet-name>
        <servlet-class>ex2.Authentication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ex2-2</servlet-name>
        <servlet-class>ex2.AServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ex3-1</servlet-name>
        <servlet-class>ex3.Authentication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ex3-2</servlet-name>
        <servlet-class>ex3.AServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ex1</servlet-name>
        <url-pattern>/ex1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ex2-1</servlet-name>
        <url-pattern>/authentication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ex2-2</servlet-name>
        <url-pattern>/app</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ex3-1</servlet-name>
        <url-pattern>/authentication2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ex3-2</servlet-name>
        <url-pattern>/app2</url-pattern>
    </servlet-mapping>
</web-app>